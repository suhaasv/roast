<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.roast">
    <uses-permission android:name="android.permission.INTERNET" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Activities.StartingActivities.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Activities.PreWritten.PreActivity">
        </activity>
        <activity android:name=".Activities.Favorites.FavActivity">
        </activity>
        <activity android:name=".Activities.Chat.ChatActivity">
        </activity>
        <activity android:name=".Activities.PreWritten.PreWrittenDetailActivity">
        </activity>
        <activity android:name=".Activities.Chat.MainChatActivity">
        </activity>
    </application>

</manifest>